<!doctype html>
<html lang="ja">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
      {% block title %}
        タイトル
      {% endblock title %}
    </title>
    <meta name="description" content="" />
    <meta name="keywords" content="" />
  </head>
  <body>
    {% block header %}ヘッダー{% endblock header %}
    {% block nav %}
      <ul>
        <li>
          <a href="{{ url_for('index') }}">トップ</a>
        </li>
        <li>
          <a href="{{ url_for('item_list') }}">商品一覧</a>
        </li>
        <li>
          <a href="{{ url_for('item_detail') }}">商品詳細</a>
        </li>
        <li>
          <a href="{{ url_for('dynamic_default', value='hello') }}"
            >コンバーターなし</a
          >
        </li>
        <li>
          <a href="{{ url_for('dynamic_int', number=100) }}"
            >コンバーターあり</a
          >
        </li>
        <li>
          <a
            href="{{ url_for('dynamic_converter_multiple', value='hello', number=100) }}"
            >コンバーターあり複数値渡し</a
          >
        </li>
      </ul>
    {% endblock nav %}
    {% block content %}内容{% endblock content %}
    {% block footer %}<ul>
      {% block footer_top %}
        <li>
          <a href="{{ url_for('index') }}">トップ</a>
        </li>
      {% endblock footer_top %}
    </ul>{% endblock footer %}
  </body>
</html>
